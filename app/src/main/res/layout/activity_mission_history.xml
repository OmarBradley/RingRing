        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</io.github.francoiscampbell.circlelayout.CircleLayout>