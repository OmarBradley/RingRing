<?xml version="1.0" encoding="utf-8"?>
<io.github.francoiscampbell.circlelayout.CircleLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cl="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="80dp"
    android:paddingLeft="13dp"
    android:paddingRight="13dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    cl:cl_angleOffset="45"
    cl:cl_centerView="@+id/image_keyword_center"
    cl:cl_direction="clockwise">

    <ImageView
        android:id="@+id/image_keyword_center"
        android:layout_width="119dp"
        android:layout_height="119dp"
        android:src="@drawable/keyword_center_image" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_pretty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingEnd="3dp" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_good"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_handsome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_love"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_cute"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_good_night"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <olab.ringring.main.mymenu.missionhistory.customview.KeywordView
        android:id="@+id/view_keyword_beating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</io.github.francoiscampbell.circlelayout.CircleLayout>